{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "name": "Launch via yarn",
            "request": "launch",
            "runtimeArgs": [
                "dev"
            ],
            "runtimeExecutable": "yarn",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env.local",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "protocol": "inspector"
        },
    ]
}